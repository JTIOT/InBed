
@{
    ViewBag.Title = "Welcome";
    Layout = "~/Areas/Adm/Views/Shared/_Layout_Body.cshtml";
}
@section StylesBefore{
    <link href="~/assets/css/index.css" rel="stylesheet" type="text/css" />
}
@section Scripts{
    <script type="text/javascript" src="http://api.map.baidu.com/api?v=2.0&ak=TuNY8zbjIexxzrhwfXmwDCBAmDuCUdxX"></script>
    <script src="~/assets/js/mapv.min.js" type="text/javascript"></script>
    <script src="~/assets/js/jquery-1.11.2.min.js" type="text/javascript"></script>
    <script src="~/assets/js/echarts.js" type="text/javascript"></script>
    <script src="~/assets/js/index-map.js" type="text/javascript"></script>
    <script src="~/assets/js/index-echart.js" type="text/javascript"></script>
    <script type="text/javascript">

        function indexData() {
            $.ajax({
                type: "get",
                url: "/Control/GetElderCount",
                dataType: "json",
                success: function (result) {
                    $('#sumCount').html(result.SumCount + '人');
                    $('#newCount').html(result.NewCount + '人/天');
                    $('.system_number').show();
                }
            })
        }
        indexData();
    </script>
}

